---
import { translations } from "@/i18n/config";

import Layout from "@/layouts/Layout.astro";
import Hero from "@/components/Hero.astro";
import AreasOfImpact from "@/components/AreasOfImpact.astro";
import Glaci from "@/components/Glaci.astro";
import Work from "@/components/Work.astro";
import Challenge from "@/components/Challenge.astro";
import OurModel from "@/components/OurModel.astro";
import Differentiation from "@/components/Differentiation.astro";
import Value from "@/components/Value.astro";
import Vision from "@/components/Vision.astro";

export function getStaticPaths() {
  return Object.keys(translations).map((lang) => ({ params: { lang } }));
}
---

<Layout title="G L A C I">
  <Hero />
  <Challenge />
  <Glaci />
  <OurModel />
  <AreasOfImpact />
  <Value />
  <Work />
  <Differentiation />
  <Vision />
</Layout>
